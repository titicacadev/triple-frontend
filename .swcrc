{
  "$schema": "https://json.schemastore.org/swcrc",
  "module": {
    "type": "commonjs"
  },
  "sourceMaps": true,
  "jsc": {
    "target": "es2016",
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "throwIfNamespace": true
      }
    }
  },
  "exclude": [".*\\.(test|spec)\\.(js|ts)x?$"]
}

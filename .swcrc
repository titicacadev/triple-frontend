{
  "$schema": "https://json.schemastore.org/swcrc",
  "module": {
    "type": "es6"
  },
  "sourceMaps": true,
  "jsc": {
    "target": "es2015",
    "parser": {
      "syntax": "typescript",
      "tsx": true
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "throwIfNamespace": false
      }
    },
    "experimental": {
      "plugins": [["@swc/plugin-styled-components", {}]]
    }
  }
}

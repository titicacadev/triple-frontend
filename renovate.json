{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchManagers": ["npm"],
      "matchDepTypes": ["engines"],
      "enabled": false
    },
    {
      "matchSourceUrls": ["https://github.com/titicacadev/triple-frontend"],
      "enabled": false
    },
    {
      "groupName": "SWC",
      "matchPackagePrefixes": ["@swc/"]
    },
    {
      "matchPackageNames": ["firebase", "nx-cloud"],
      "enabled": false
    }
  ]
}

  },
  "devDependencies": {
    "@titicaca/triple-web": "workspace:*",
    "@types/ua-parser-js": "^0.7.39",
    "next": "^13.5.6",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "@titicaca/triple-web": "*",
    "next": "^13.4",
    "react": "^18.0"
  }
}

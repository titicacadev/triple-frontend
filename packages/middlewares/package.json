{
  "name": "@titicaca/middlewares",
  "version": "13.15.0",
  "description": "Triple Web Application Middleware modules",
  "license": "MIT",
  "homepage": "https://github.com/titicacadev/triple-frontend/tree/main/packages/middleware",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/titicacadev/triple-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/titicacadev/triple-frontend/issues"
  },
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "publishConfig": {
    "main": "lib/index.cjs",
    "module": "lib/index.js",
    "types": "lib/index.d.ts",
    "exports": {
      ".": {
        "types": "./lib/index.d.ts",
        "import": "./lib/index.js",
        "require": "./lib/index.cjs"
      }
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:types",
    "build:esm": "swc src -d lib --ignore **/*.stories.\\(ts\\|tsx\\) --ignore **/*.test.\\(ts\\|tsx\\) --ignore **/*.spec.\\(ts\\|tsx\\) --config-file ../../.swcrc --strip-leading-paths",
    "build:cjs": "swc src -d lib --ignore **/*.stories.\\(ts\\|tsx\\) --ignore **/*.test.\\(ts\\|tsx\\) --ignore **/*.spec.\\(ts\\|tsx\\) --config-file ../../.swcrc --strip-leading-paths --config module.type=commonjs --out-file-extension cjs",
    "build:types": "tsc -p tsconfig.build.json",
    "lint:es": "eslint src",
    "lint:es:fix": "eslint src --fix",
    "lint:etc": "prettier src --check",
    "lint:etc:fix": "prettier src --write"
  },
  "lint-staged": {
    "*": [
      "prettier --check"
    ],
    "*.{js,ts,tsx}": [
      "eslint"
    ]
  },
  "dependencies": {
    "set-cookie-parser": "^2.6.0",
    "universal-cookie": "^4.0.4",
    "semver": "^7.6.2"
  },
  "devDependencies": {
    "@titicaca/fetcher": "workspace:*",
    "@titicaca/triple-web-utils": "workspace:*",
    "@types/semver": "^7.5.8",
    "@types/set-cookie-parser": "^2.4.7",
    "react": "^18.2.0",
    "next": "^13.5.6"
  },
  "peerDependencies": {
    "@titicaca/fetcher": "*",
    "@titicaca/triple-web-utils": "*",
    "react": "^18.0",
    "next": "^13.4"
  }
}

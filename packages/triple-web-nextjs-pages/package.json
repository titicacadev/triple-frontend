{
  "name": "@titicaca/triple-web-nextjs-pages",
  "version": "13.7.0",
  "license": "MIT",
  "homepage": "https://github.com/titicacadev/triple-frontend/tree/main/packages/triple-web-nextjs-pages",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/titicacadev/triple-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/titicacadev/triple-frontend/issues"
  },
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "publishConfig": {
    "main": "lib/index.cjs",
    "module": "lib/index.js",
    "types": "lib/index.d.ts",
    "exports": {
      ".": {
        "types": "./lib/index.d.ts",
        "import": "./lib/index.js",
        "require": "./lib/index.cjs"
      }
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "pnpm run build:esm && pnpm run build:types",
    "build:esm": "swc src -d lib --ignore **/*.stories.\\(ts\\|tsx\\) --ignore **/*.test.\\(ts\\|tsx\\) --ignore **/*.spec.\\(ts\\|tsx\\) --config-file ../../.swcrc --strip-leading-paths",
    "build:cjs": "swc src -d lib --ignore **/*.stories.\\(ts\\|tsx\\) --ignore **/*.test.\\(ts\\|tsx\\) --ignore **/*.spec.\\(ts\\|tsx\\) --config-file ../../.swcrc --strip-leading-paths --config module.type=commonjs --out-file-extension cjs",
    "build:types": "tsc -p tsconfig.build.json",
    "lint:es": "eslint src",
    "lint:es:fix": "eslint src --fix",
    "lint:etc": "prettier src --check",
    "lint:etc:fix": "prettier src --write"
  },
  "lint-staged": {
    "*": [
      "prettier --check"
    ],
    "*.{js,ts,tsx}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@titicaca/fetcher": "workspace:*",
    "@titicaca/triple-web-utils": "workspace:*",
    "@titicaca/view-utilities": "workspace:*",
    "qs": "^6.12.1",
    "ua-parser-js": "^1.0.37",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@titicaca/triple-web": "workspace:*",
    "@types/qs": "^6.9.15",
    "@types/ua-parser-js": "^0.7.39",
    "next": "^13.5.6"
  },
  "peerDependencies": {
    "@titicaca/triple-web": "*",
    "next": "^13.4"
  }
}

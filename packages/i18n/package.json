{
  "name": "@titicaca/i18n",
  "version": "12.19.1",
  "description": "Triple-frontend Internalization",
  "license": "MIT",
  "homepage": "https://github.com/titicacadev/triple-frontend/tree/main/packages/i18n",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/titicacadev/triple-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/titicacadev/triple-frontend/issues"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "app-directory.d.ts",
    "app-directory.js",
    "client.d.ts",
    "client.js",
    "middleware-utils.d.ts",
    "middleware-utils.js",
    "server.d.ts",
    "server.js"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:types",
    "build:cjs": "swc src -d lib --config-file ../../.swcrc",
    "build:types": "tsc -p tsconfig.build.json",
    "lint:es": "eslint src",
    "lint:es:fix": "eslint src --fix",
    "lint:etc": "prettier src --check",
    "lint:etc:fix": "prettier src --write"
  },
  "lint-staged": {
    "*": [
      "prettier --check"
    ],
    "*.{js,ts,tsx}": [
      "eslint"
    ]
  },
  "dependencies": {
    "node-fetch": "2.6.11"
  },
  "devDependencies": {
    "@types/node-fetch": "2.x"
  },
  "peerDependencies": {
    "react": "*",
    "next": "*",
    "i18next": "*"
  }
}

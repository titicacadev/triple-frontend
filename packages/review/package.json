    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@titicaca/action-sheet": "^8.1.2",
    "@titicaca/core-elements": "^8.1.2",
    "@titicaca/fetcher": "^8.1.2",
    "@titicaca/intersection-observer": "^8.1.2",
    "@titicaca/modals": "^8.1.2",
    "@titicaca/react-hooks": "^8.1.2",
    "@titicaca/react-triple-client-interfaces": "^8.1.2",
    "@titicaca/router": "^8.1.2",
    "@titicaca/type-definitions": "^8.1.2",
    "@titicaca/ui-flow": "^8.1.2",
    "@titicaca/view-utilities": "^8.1.2",
    "graphql-request": "^4.3.0",
    "moment": "^2.24.0",
    "qs": "^6.9.0",
    "react-query": "^3.39.1",
    "scroll-to-element": "^2.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",

{
  "name": "@titicaca/review",
  "version": "12.15.0",
  "description": "UI Components for User Reviews from Triple Service",
  "license": "MIT",
  "homepage": "https://github.com/titicacadev/triple-frontend/tree/main/packages/review",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/titicacadev/triple-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/titicacadev/triple-frontend/issues"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:types",
    "build:cjs": "swc src -d lib --config-file ../../.swcrc",
    "build:types": "tsc -p tsconfig.build.json",
    "lint:es": "eslint src",
    "lint:es:fix": "eslint src --fix",
    "lint:style": "stylelint 'src/**/*.{js,ts,tsx}'",
    "lint:style:fix": "stylelint 'src/**/*.{js,ts,tsx}' --fix",
    "lint:etc": "prettier src --check",
    "lint:etc:fix": "prettier src --write",
    "generate": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@titicaca/action-sheet": "^12.15.0",
    "@titicaca/core-elements": "^12.15.0",
    "@titicaca/fetcher": "^12.15.0",
    "@titicaca/intersection-observer": "^12.15.0",
    "@titicaca/modals": "^12.15.0",
    "@titicaca/react-hooks": "^12.15.0",
    "@titicaca/router": "^12.15.0",
    "@titicaca/type-definitions": "^12.15.0",
    "@titicaca/ui-flow": "^12.15.0",
    "@titicaca/view-utilities": "^12.15.0",
    "graphql": "^16.6.0",
    "graphql-request": "^4.3.0",
    "moment": "^2.29.4",
    "qs": "^6.11.1",
    "react-query": "^3.39.3",
    "scroll-to-element": "^2.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.5",
    "@graphql-codegen/typescript": "2.8.8",
    "@graphql-codegen/typescript-operations": "2.5.13",
    "@graphql-codegen/typescript-react-query": "^3.6.2",
    "@titicaca/i18n": "^12.15.0",
    "@titicaca/next-i18next": "13.8.5",
    "@titicaca/react-contexts": "^12.15.0",
    "@titicaca/react-triple-client-interfaces": "^12.15.0",
    "@types/qs": "^6.9.7",
    "csstype": "^3.1.2"
  },
  "peerDependencies": {
    "@titicaca/next-i18next": "*",
    "@titicaca/react-contexts": "*",
    "@titicaca/react-triple-client-interfaces": "*",
    "react": "^18.0",
    "react-dom": "^18.0",
    "styled-components": "^5.3.9"
  }
}

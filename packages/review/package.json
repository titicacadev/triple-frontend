{
  "name": "@titicaca/review",
  "version": "13.2.2",
  "description": "UI Components for User Reviews from Triple Service",
  "license": "MIT",
  "homepage": "https://github.com/titicacadev/triple-frontend/tree/main/packages/review",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/titicacadev/triple-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/titicacadev/triple-frontend/issues"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:types",
    "build:cjs": "swc src -d lib --config-file ../../.swcrc",
    "build:types": "tsc -p tsconfig.build.json",
    "lint:es": "eslint src",
    "lint:es:fix": "eslint src --fix",
    "lint:style": "stylelint 'src/**/*.{js,ts,tsx}'",
    "lint:style:fix": "stylelint 'src/**/*.{js,ts,tsx}' --fix",
    "lint:etc": "prettier src --check",
    "lint:etc:fix": "prettier src --write",
    "generate": "graphql-codegen"
  },
  "lint-staged": {
    "*": [
      "prettier --check"
    ],
    "*.{js,ts,tsx}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@titicaca/action-sheet": "^13.2.2",
    "@titicaca/core-elements": "^13.2.2",
    "@titicaca/fetcher": "^13.2.2",
    "@titicaca/intersection-observer": "^13.2.2",
    "@titicaca/modals": "^13.2.2",
    "@titicaca/react-hooks": "^13.2.2",
    "@titicaca/router": "^13.2.2",
    "@titicaca/type-definitions": "^13.2.2",
    "@titicaca/ui-flow": "^13.2.2",
    "@titicaca/view-utilities": "^13.2.2",
    "graphql": "^16.6.0",
    "graphql-request": "^6.1.0",
    "moment": "^2.29.4",
    "qs": "^6.11.2",
    "scroll-to-element": "^2.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/typescript": "4.0.1",
    "@graphql-codegen/typescript-generic-sdk": "^3.1.0",
    "@graphql-codegen/typescript-operations": "4.0.1",
    "@titicaca/i18n": "^13.2.2",
    "@titicaca/next-i18next": "13.8.5",
    "@titicaca/react-contexts": "^13.2.2",
    "@titicaca/react-triple-client-interfaces": "^13.2.2",
    "@types/qs": "^6.9.7",
    "csstype": "^3.1.2",
    "react-query": "^3.39.3"
  },
  "peerDependencies": {
    "@titicaca/next-i18next": "*",
    "@titicaca/react-contexts": "*",
    "@titicaca/react-triple-client-interfaces": "*",
    "react": "^18.0",
    "react-dom": "^18.0",
    "react-query": "^3.39.3",
    "styled-components": "^5.3.9"
  }
}
